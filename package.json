{
  "name": "marquelmedia.assets",
  "version": "1.1.8",
  "description": "Resources for MARQUELMEDIA deployments",
  "author": "@eatmycode",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "repo": "echo -e \"\n\nASSETS\"",
    "pull": "git pull && bun install",
    "commit": "sh -c 'if [[ -n $(git status --porcelain) ]]; then git add . && echo ${0} ${*} | git commit -aF - && git push; fi'",
    "precommit": "bun run --silent repo && git checkout main && bun run ci/env.ts"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "canvas": "^2.11.2",
    "potrace": "^2.1.8",
    "sharp": "^0.31.3",
    "sharp-ico": "^0.1.5",
    "uglify-js": "^3.17.4",
    "uglifycss": "^0.0.29"
  },
  "repository": "git@github.com:marquelmedia/assets.git",
  "devDependencies": {}
}